{
  "name": "collectype",
  "version": "0.8.4",
  "description": "CollecType is a modern, type-safe collection utility for TypeScript. Effortlessly filter, sort, and transform arrays of any type using a fluent, chainable API.",
  "keywords": [
    "typescript",
    "collection",
    "filter",
    "sort",
    "predicate"
  ],
  "license": "MIT",
  "author": "Marc-Antoine Duhaime",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maduhaime/collectype.git"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./enums": {
      "import": "./dist/enums/index.js",
      "types": "./dist/enums/index.d.ts"
    },
    "./types": {
      "import": "./dist/types/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./utils/factory": {
      "import": "./dist/utils/factory/index.js",
      "types": "./dist/utils/factory/index.d.ts"
    },
    "./utils/predicates": {
      "import": "./dist/utils/predicates/index.js",
      "types": "./dist/utils/predicates/index.d.ts"
    },
    "./utils/primitives": {
      "import": "./dist/utils/primitives/index.js",
      "types": "./dist/utils/primitives/index.d.ts"
    }
  },
  "scripts": {
    "build": "rimraf dist && tsc --project tsconfig.build.json",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "docs": "rimraf docs && typedoc",
    "docs:deploy": "npm run docs && gh-pages -d docs -f"
  },
  "devDependencies": {
    "@types/node": "^24.3.1",
    "@vitest/coverage-v8": "^3.2.4",
    "gh-pages": "^6.3.0",
    "rimraf": "^6.0.1",
    "typedoc": "^0.28.12",
    "typedoc-plugin-markdown": "^4.8.1",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
